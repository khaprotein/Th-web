
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <!-- theme meta -->
    <meta name="theme-name" content="quixlab" />

    <title>Quản trị viên</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon.png">
    <!-- Pignose Calender -->
    <link href="~/Areas/ContentAdmin/plugins/pg-calendar/css/pignose.calendar.min.css" rel="stylesheet" />
    <!-- Chartist -->
    <link href="~/Areas/ContentAdmin/plugins/chartist/css/chartist.min.css" rel="stylesheet" />
    <link href="~/Areas/ContentAdmin/plugins/chartist-plugin-tooltips/css/chartist-plugin-tooltip.css" rel="stylesheet" />
    <!-- Custom Stylesheet -->
    <link href="~/Areas/ContentAdmin/css/style.css" rel="stylesheet" />

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <div class="nav-header">
            <div class="brand-logo">
                <a href="@Url.Action("Index","ThongKe")">
                    <b class="logo-abbr"><img src="~/Areas/ContentAdmin/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/Areas/ContentAdmin/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/Areas/ContentAdmin/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->
        <!--**********************************
            Header start
        ***********************************-->
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
               
                <div class="header-left">
                    @using (Html.BeginForm("TimKiem", "TimKiem_Admin"))
                    {
                        <div class="input-group icons">
                            <div class="input-group-prepend">
                                <span class="input-group-text bg-transparent border-0 pr-2 pr-sm-3" id="basic-addon1"><i class="mdi mdi-magnify"></i></span>
                            </div>
                            <input type="search" name="sTuKhoa" class="form-control" placeholder="Tìm kiếm" aria-label="Tìm kiếm">
                            @*<div class="drop-down animated flipInX d-md-none">
                    <form action="#">
                        <input type="text" class="form-control" placeholder="Tìm kiếm">
                    </form>
                </div>*@
                        </div>
                    }
                    </div>
                
                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown d-none d-md-flex">
                            <a href="javascript:void(0)" class="log-user" data-toggle="dropdown">
                                <span>Quản trị viên</span>
                            </a>
                        </li>
                        <li class="icons dropdown">
                            <div class="user-img c-pointer position-relative" data-toggle="dropdown">
                                <span class="activity active"></span>
                                <img src="~/Areas/ContentAdmin/images/users/avt_admin.png" height="40" width="40" alt="" />
                            </div>
                            <div class="drop-down dropdown-profile animated fadeIn dropdown-menu">
                                <div class="dropdown-content-body">
                                    <ul>
                                        <li><a href="~/Admin/HomeAdmin/DangXuat"><i class="icon-key"></i> <span>Logout</span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->
        <!--**********************************
            Sidebar start
        ***********************************-->
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">Tổng quan</li>
                    <li>
                        <a class="has-arrow" href="javascript:void()" aria-expanded="false">
                            <i class="icon-speedometer menu-icon"></i><span class="nav-text">Quản lý</span>
                        </a>
                        <ul aria-expanded="false">
                            <li><a href="@Url.Action("DanhSachCoSo", "CoSo")">Cơ sở</a></li>
                            <li><a href="@Url.Action("DanhSachNhanVien", "NhanVien")">Nhân viên</a></li>
                            <li><a href="@Url.Action("DanhSachSan", "San")">Sân</a></li>
                            <li><a href="@Url.Action("DanhSachPhanCong", "PhanCong")">Phân công</a></li>
                            <li><a href="@Url.Action("DanhSachKhachHang", "KhachHang")">Khách hàng</a></li>
                            <li><a href="@Url.Action("DanhSachLichDat", "HomeAdmin")">Lịch đặt</a></li>


                            <!-- <li><a href="./index-2.html">Home 2</a></li> -->
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <!--**********************************
            Sidebar end
        ***********************************-->
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid mt-3">
                <div class="row">
                    @RenderSection("ThongKe", false)
                    @RenderBody()
                </div>
            </div>
            <!-- #/ container -->
        </div>
        <!--**********************************
            Content body end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Footer start
    ***********************************-->
    <div class="footer">
        <div class="copyright">
            <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a> 2018</p>
        </div>
    </div>
    <!--**********************************
        Footer end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <script src="~/Areas/ContentAdmin/plugins/common/common.min.js"></script>
    <script src="~/Areas/ContentAdmin/js/custom.min.js"></script>
    <script src="~/Areas/ContentAdmin/js/settings.js"></script>
    <script src="~/Areas/ContentAdmin/js/gleek.js"></script>
    <script src="~/Areas/ContentAdmin/js/styleSwitcher.js"></script>

    <!-- Chartjs -->
    <script src="~/Areas/ContentAdmin/plugins/chart.js/Chart.bundle.min.js"></script>
    <!-- Circle progress -->
    <script src="~/Areas/ContentAdmin/plugins/circle-progress/circle-progress.min.js"></script>
    <!-- Datamap -->
    <script src="~/Areas/ContentAdmin/plugins/d3v3/index.js"></script>
    <script src="~/Areas/ContentAdmin/plugins/topojson/topojson.min.js"></script>
    <script src="~/Areas/ContentAdmin/plugins/datamaps/datamaps.world.min.js"></script>
    <!-- Morrisjs -->
    <script src="~/Areas/ContentAdmin/plugins/raphael/raphael.min.js"></script>
    <script src="~/Areas/ContentAdmin/plugins/morris/morris.min.js"></script>
    <!-- Pignose Calender -->
    <script src="~/Areas/ContentAdmin/plugins/moment/moment.min.js"></script>
    <script src="~/Areas/ContentAdmin/plugins/pg-calendar/js/pignose.calendar.min.js"></script>
    <!-- ChartistJS -->
    <script src="~/Areas/ContentAdmin/plugins/chartist/js/chartist.min.js"></script>
    <script src="~/Areas/ContentAdmin/plugins/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js"></script>



    <script src="~/Areas/ContentAdmin/js/dashboard/dashboard-1.js"></script>

</body>

</html>
